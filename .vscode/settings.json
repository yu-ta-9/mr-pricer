{
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.fixAll.eslint": true
  },
  "stylelint.validate": ["css", "scss"],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "cSpell.words": [],
  "peacock.color": "#A7CCFC",
  "i18n-ally.localesPaths": ["public/locales"],
  "i18n-ally.keystyle": "nested",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#d8e9fe",
    "activityBar.background": "#d8e9fe",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#fb64a7",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d8e9fe",
    "statusBar.background": "#a7ccfc",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#76affa",
    "statusBarItem.remoteBackground": "#a7ccfc",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#a7ccfc",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a7ccfc99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "WillLuke.nextjs.addTypesOnSave": true,
  "WillLuke.nextjs.hasPrompted": true
}
