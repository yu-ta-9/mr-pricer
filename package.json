{
  "name": "mr-pricer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:js": "eslint **/*.{ts,tsx}",
    "lint:js:fix": "eslint --fix **/*.{ts,tsx}",
    "ts-test": "tsc --noEmit",
    "db:studio": "dotenv -e .env.local npx prisma studio",
    "db:generate:local": "dotenv -e .env.local npx prisma generate",
    "db:push:local": "dotenv -e .env.local npx prisma db push",
    "db:migrate:local": "dotenv -e .env.local npx prisma migrate dev",
    "db:migrate:resolve:local": "dotenv -e .env.local npx prisma migrate resolve",
    "db:migrate:reset:local": "dotenv -e .env.local npx prisma migrate reset",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@aws-sdk/s3-request-presigner": "^3.359.0",
    "@aws-sdk/signature-v4-crt": "^3.357.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.4",
    "crypto-random-string": "^5.0.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "flowbite": "^1.6.5",
    "install": "^0.13.0",
    "next": "13.4.4",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.0-dev.20230606",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.20",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.0.27",
    "@storybook/testing-library": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "8.4.24",
    "prisma": "^4.15.0",
    "storybook": "^7.0.20"
  }
}
